{
  "name": "yournextstore-with-payloadcms",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.3",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@vercel/speed-insights"
    ],
    "onlyBuiltDependencies": [
      "@vercel/speed-insights",
      "sharp"
    ]
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
