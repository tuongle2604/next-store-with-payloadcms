{
  "name": "@repo/cms",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "next build",
    "start": "next start",
    "build:compile": "cross-env NODE_OPTIONS=--no-deprecation next build --experimental-build-mode compile",
    "build:generate": "cross-env NODE_OPTIONS=--no-deprecation next build --experimental-build-mode generate",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation  next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation NODE_ENV=production next dev",
    "prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env  NODE_ENV=production NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "update-product-collection": "payload run scripts/update-product-collection.js"
  },
  "dependencies": {
    "@payloadcms/admin-bar": "3.69.0",
    "@payloadcms/db-postgres": "3.69.0",
    "@payloadcms/db-vercel-postgres": "^3.69.0",
    "@payloadcms/email-nodemailer": "3.69.0",
    "@payloadcms/live-preview-react": "3.69.0",
    "@payloadcms/next": "3.69.0",
    "@payloadcms/payload-cloud": "3.69.0",
    "@payloadcms/plugin-form-builder": "3.69.0",
    "@payloadcms/plugin-nested-docs": "3.69.0",
    "@payloadcms/plugin-redirects": "3.69.0",
    "@payloadcms/plugin-search": "3.69.0",
    "@payloadcms/plugin-seo": "3.69.0",
    "@payloadcms/richtext-lexical": "3.69.0",
    "@payloadcms/sdk": "^3.69.0",
    "@payloadcms/storage-s3": "3.69.0",
    "@payloadcms/ui": "3.69.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-email/components": "^0.4.0",
    "@repo/schemas": "workspace:*",
    "@repo/shared-data": "workspace:*",
    "@tanstack/react-query": "^5.85.0",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "2.1.1",
    "cross-env": "7.0.3",
    "geist": "^1.4.2",
    "graphql": "^16.11.0",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.509.0",
    "next": "15.4.10",
    "next-sitemap": "^4.2.3",
    "nodemailer": "^7.0.5",
    "payload": "^3.63.0",
    "prism-react-renderer": "^2.4.1",
    "qs-esm": "7.0.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.56.3",
    "sharp": "0.34.1",
    "slugify": "^1.6.6",
    "stripe": "18.0.0",
    "tailwind-merge": "3.3.0",
    "tailwindcss-animate": "1.0.7",
    "zod": "4.0.8",
    "zustand": "5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@repo/ts-config": "workspace:*",
    "@tailwindcss/typography": "0.5.16",
    "@types/escape-html": "1.0.4",
    "@types/node": "22.15.17",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "autoprefixer": "10.4.21",
    "copyfiles": "2.4.1",
    "eslint": "9.27.0",
    "eslint-config-next": "15.3.2",
    "postcss": "8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": "^20.11.1 || ^22.0.0",
    "pnpm": "^9 || ^10"
  }
}
